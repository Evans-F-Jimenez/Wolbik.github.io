<krpano version="1.21" title="Edificio AH">

	<include url="skin/vtourskin.xml" />
	<include url="%VIEWER%/plugins/webvr.xml" />
	<include url="plugins/circle_hotspots.xml"/>

	<layer name="intro"
		type="container"
		keep="true"
		width="100%"
		height="100%"
		bgcolor="0x000000"
		bgalpha="0.8"
		zorder="9999"
		enabled="true"
		bgcapture="true"
		vr="true"
		onloaded="on_intro_loaded()"
		onclick="on_intro_click()">
		<layer name="intro_logo" 
			url="skin/logo.png" 
			align="center" 
			y="-70" 
			scale="0.2"
			enabled="false"
			alpha="0"
			vr="true"
		/>
		<layer name="intro_guide" 
			url="skin/help_desktop.png" 
			align="center" y="80" 
			scale="1"
			enabled="false"
			alpha="0"
			vr="true"
		/>
	</layer>


	<action name="on_intro_click">
		tween(layer[intro].alpha, 0);
		set(layer[intro].enabled, false);
	</action>

	<action name="on_intro_loaded">
		delayedcall(2,
			tween(layer[intro_logo].alpha, 1);
			tween(layer[intro_guide].alpha, 1);
		);
		
	</action>

	<layer name="bg_ascensor"
		type="container"
		keep="true"
		width="100%"
		height="100%"
		bgcolor="0x000000"
		bgalpha="0.8"
		zorder="9999"
		bgcapture="true"
		visible="false"
		vr="true"
		onclick="close_btns_ascensor()">

		<layer name="btns_ascensor"
			keep="true"
			align="center"
			x="0" y="0"
			url="skin/bgblackimage.png" 
			scale="1.0"
			vr="true">

			<!-- Layers hijos con acciones -->
			<layer name="btn1" 
				url="skin/button.png" 
				scale="0.05"
				align="lefttop" 
				x="10" y="10"
				vr="true"
				onclick="accion_imagen1()" />
			<layer name="btn2" 
				url="skin/button.png"
				scale="0.05"
				align="lefttop" 
				x="50" y="50" 
				vr="true"
				onclick="accion_imagen2()" />
		</layer>
	</layer>

	<action name="accion_imagen1">
		loadscene(scene_R0010005, null, MERGE);
		close_btns_ascensor();
	</action>

	<action name="accion_imagen2">
		loadscene(scene_R0010006, null, MERGE);
		close_btns_ascensor();
	</action>

	<action name="open_btns_ascensor">
		trace("open_btns_ascensor");
		set(layer[bg_ascensor].visible, true);
		set(layer[bg_ascensor].capture, true);
		set(layer[btns_ascensor].onclick, close_btns_ascensor);
	</action>

	<action name="close_btns_ascensor">
		set(layer[bg_ascensor].visible, false);
		set(layer[bg_ascensor].capture, false);
		set(layer[tour].enabled, true);
	</action>

	<style name="infospot"
		renderer="webgl"
		type="text"
		css="color:#FFFFFF; font-size:16px; font-weight:bold; text-align:center;"
		width="80" height="80"
		bgroundedge="42"
		bgalpha="0.75"
		bgcolor="0x555555"
		bgborder="1 0xFFFFFF 1"
		bgshadow="0 0 16 0x0 0.5"
		txtshadow="0 0 3 0x0 1.0"
		mergedalpha="false"
		vcenter="true"
		oversampling="2"
		mipmapping="true"
		zoom="true"
		capture="false"
		/>

